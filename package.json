  "repository": {
    "type": "git",
    "url": "https://github.com/phrase/react-i18next-phraseapp.git"
  },
  "keywords": [
    "phraseapp",
    "i18n",
    "l10n",
    "react",
    "i18next",
    "localization",
    "translation"
  ],
  "author": "Manuel Boy <support@phraseapp.com> (https://phraseapp.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phrase/react-i18next-phraseapp/issues",
    "email": "support@phraseapp.com"
  },
  "devDependencies": {
    "@types/react": "^15.6.19",
    "@types/react-dom": "^0.14.23",
    "@types/react-i18next": "^1.7.12",
    "@types/react-test-renderer": "^15.5.7",
    "awesome-typescript-loader": "^3.5.0",
    "debug": "^2.6.9",
    "enzyme": "^2.8.0",
    "hoist-non-react-statics": "^1.2.0",
    "i18next": "^7.1.3",
    "jest": "^23.5.0",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.2",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-i18next": "^2.2.3",
    "react-test-renderer": "^15.6.2",
    "regenerator-runtime": "^0.10.3",
    "sshpk": "^1.13.2",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "peerDependencies": {
    "i18next": ">=7.1.3",
    "react": "^15.4.2",
    "react-i18next": ">=2.2.3"
  },
  "files": [
    "dist/react-i18next-phraseapp.js"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
