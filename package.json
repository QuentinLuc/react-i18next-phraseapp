{
  "name": "react-i18next-phraseapp",
  "version": "1.0.0",
  "description": "Connect react-i18next with the PhraseApp In-Context Editor",
  "main": "./dist/react-i18next-phraseapp.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "webpack --config webpack.config.js",
    "test": "jest",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phrase/react-i18next-phraseapp.git"
  },
  "keywords": [
    "phraseapp",
    "i18n",
    "l10n",
    "react",
    "i18next",
    "translation"
  ],
  "author": "Manuel Boy",
  "license": "MIT",
  "dependencies": {
    "hoist-non-react-statics": "^1.2.0",
    "i18next": "^7.1.3",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-i18next": "^2.2.3"
  },
  "bugs": {
    "url" : "https://github.com/phrase/react-i18next-phraseapp/issues",
    "email" : "support@phraseapp.com"
  },
  "devDependencies": {
    "@types/react": "^15.0.22",
    "@types/react-dom": "^0.14.23",
    "@types/react-i18next": "^1.7.7",
    "@types/react-test-renderer": "^15.4.3",
    "awesome-typescript-loader": "^3.1.2",
    "enzyme": "^2.8.0",
    "jest": "^19.0.2",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "regenerator-runtime": "^0.10.3",
    "rimraf": "^2.6.1",
    "ts-jest": "^19.0.10",
    "typescript": "^2.2.2",
    "webpack": "^2.3.3"
  },
  "peerDependencies": {
    "i18next": "^7.1.3",
    "react": "^15.4.2",
    "react-i18next": "^2.2.3"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
