    "regenerator-runtime": "^0.10.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^19.0.10",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0"
  },
  "peerDependencies": {
    "i18next": ">=7.1.3",
    "react": "^15.4.2",
    "react-i18next": ">=2.2.3"
  },
  "files": [
    "dist/react-i18next-phraseapp.js"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
