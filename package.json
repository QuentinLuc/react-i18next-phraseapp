  "bugs": {
    "url": "https://github.com/phrase/react-i18next-phraseapp/issues",
    "email": "support@phraseapp.com"
  },
  "devDependencies": {
    "hoist-non-react-statics": "^1.2.0",
    "i18next": "^7.1.3",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-i18next": "^2.2.3",
    "@types/react": "^15.6.14",
    "@types/react-dom": "^0.14.23",
    "@types/react-i18next": "^1.7.11",
    "@types/react-test-renderer": "^15.5.6",
    "awesome-typescript-loader": "^3.4.1",
    "enzyme": "^2.8.0",
    "jest": "^19.0.2",
    "react-addons-test-utils": "^15.6.2",
